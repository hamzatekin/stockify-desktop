{
  "name": "vite-reactts-electron-starter",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Starter using Vite 2+ React +Typescript + Electron for fast prototyping.",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/carousel": "^5.6.1",
    "@mantine/core": "^5.6.1",
    "@mantine/dates": "^5.6.1",
    "@mantine/form": "^5.6.1",
    "@mantine/hooks": "^5.6.1",
    "@mantine/modals": "^5.6.1",
    "@mantine/notifications": "^5.6.1",
    "@mantine/nprogress": "^5.6.1",
    "@tanstack/react-query": "^4.13.0",
    "@vitejs/plugin-react": "^2.1.0",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "electron-is-dev": "^2.0.0",
    "embla-carousel-react": "^7.0.3",
    "http-status-codes": "^2.2.0",
    "mantine-datatable": "^1.7.9",
    "prisma": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "vite": "^3.1.8",
    "zustand": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.5.0",
    "electron": "^21.2.0",
    "electron-builder": "^23.6.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "postcss": "^8.4.18",
    "prettier": "2.7.1",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
